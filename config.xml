<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.chapter6324530" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>MyWeatherApp</name>
  <description>
        一款天气应用
    </description>
  <author email="khadron@163.com" href="https://github.com/Khadron">
      Khadron
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
